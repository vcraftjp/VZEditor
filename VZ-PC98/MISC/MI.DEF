63 [F08] "矩形ブロック記憶"
	m=#k, &01 cm=48,

1:	(s)?.
	(mb&4)??{ &o(m) . }
	&s
	x=-1, (kx>xb)? x=kx,
	n=ln, #b #B (ln<n)?{ n=ln, #B }
	b=$e, (ln==n)? >B
:A	#< #s #<
:B	&x(xb) #b mb=b,
	(x<0)? #> ? &x(x)
