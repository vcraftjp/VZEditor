	&d

64 [F09] "矩形ブロック挿入"
	?.
	&Bm (r==-1)?. (r&4)??{ #i . }
	&s
	ei[-, ek[-,
	x=lx,
:A	(ct <= 1)?{ #> &#68(x) #> }
	#i r[, #x (r)?{ #> #m }
	r], (r&8)?? >A
	ek], ei],
	&d cm=48,

