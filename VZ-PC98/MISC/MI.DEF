10 [ESC]E "E テキストの保存と終了"
	?. (wc==1)? #83 #C (s==4)?'Y'

11 [ESC]S "S テキストのセーブ"
	?. mb-,	#S #m

12 [ESC]Q "Q テキストを放棄"
	?. #] (mr!=1)? { #C .}
	&a(37) (r>0)? { #C 'N' }

13 [ESC]O "O 編集のやり直し"
	?. (mr!=1)?.
	&a(37) (r>0)? { &s #L 'n' #P #m &d }

14 [ESC]D "D テキストの二重化"
	?. (wt)? >A
	(wn==0 || wc==tc)?{ &m(6). }
	&s sn[, sn-,
	&#Y(0) l=ln, ly[,
	#R #P #m mr=$80,
	ky], ev[, ev-, &#J(l) ev], #W
	sn], &d #Y .
:A	(mr<=1)?{ #W (mr<=1)?{ #W . }}
	#C (wt)?{ #W &#Y(0) }

15 [ESC]A "A 全テキストの保存と終了"
	?. #Q (s==4 && r)? 'Y'

16 [ESC]X "0 エディタの終了"
	?. #[

17 [ESC]I "I カーソル位置へ挿入"
	?. (mr==$80)?{ &m(36) . }
	&m("カーソル位置へファイルを読み込みます")
	n=wn, c=wc,
	#R
:A	(s)?{ &c >A }
	(wc==c)? >Z
	mr=-1,
	&s
	#W &#T(n)
:B	#W &#T(c+1)
	#b #_ (ct)?{ #> #m #x }
:C	#y (mb)?{ #W #< #i (ek)?? #B #W >C }
	#C &#T(n)
	#< #i (ek)?? #B
	(wc>c)? >B
 	&d
:Z	&m("")

